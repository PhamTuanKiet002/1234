<!DOCTYPE html>
<html>

<head>
  <title>Kiểm Tra đề 4</title>
  <link rel='stylesheet' href='/stylesheets/style.css' />
</head>

<body>
  <h1>Custommer4</h1>
  <a href="form-add"><button>Add Custommer</button></a><br><br>
  <table border="1">
    <tr>
      <th>ID</th>
      <th>IdDistrict</th>
      <th>IdVillage</th>
      <th>Address</th>
      <th>BankName</th>
      <th>BranchName</th>
      <th>ACTION</th>
    </tr>

    <!-- ejseach -->
    <% Custommer4s.forEach(item=> { %>
      <!-- <tr>
          <td>  ejsout  </td>
         </tr> -->
      <tr>
        <td>
          <%= item._id %>
        </td>
        <td>
          <%= item.IdDistrict %>
        </td>
        <td>
          <%= item.IdVillage %>
        </td>
        <td>
          <%= item.Address %>
        </td>
        <td>
          <%= item.BankName %>
        </td>
        <td>
          <%= item.BranchName %>
        </td>
<td><a href="/form-update/<%= item._id %>">Update</a>||<a href="/form-delete/<%= item._id %>">Delete</a></td>
      </tr>
      <% }) %>
  </table>
</body>

</html>